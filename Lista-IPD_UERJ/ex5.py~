#!/usr/bin/env python3

i = 16; vec = []; vec_trc = []
for i in range(0, i):
    vec.append(input("Entre o elemento da posição de número {:d} do vetor: ".format(i+1)))
for i in range(i//2, i):
    vec_trc.append(vec[i])
for i in range(0, i//2):
    vec_trc.append(vec[i])
print("Vetor original:\n", vec)
print("Vetor trocado:\n", vec_trc)
